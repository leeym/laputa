#!/bin/sh
CURL="curl -D - -s -X POST -H \"Profiling: true\""
URL="https://j419v2ozp4.execute-api.us-west-2.amazonaws.com/default/Laputa"
DATA=""
if [ $# -gt 0 ]; then
  q=$1
  shift
  DATA="q=$q"
fi
if [ $# -gt 0 ]; then
  for i in `seq $#`; do
    j=$((i-1))
    p=$1
    shift
    DATA="$DATA&p$j="
    if [ ! "$p" = "_" ]; then
      DATA="$DATA$p"
    fi
  done
fi
CMD=$CURL
if [ ! -z "$DATA" ]; then
  CMD="$CMD -d $DATA"
fi
CMD="$CMD $URL"
echo "+ $CMD" >& 2
$CMD
echo
